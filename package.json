{
  "type": "module",
  "version": "0.9.0",
  "packageManager": "pnpm@9.4.0",
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "docs:dev": "pnpm --filter docs dev",
    "test": "vitest",
    "build": "node scripts/build.js",
    "release": "bumpp package.json packages/**/package.json",
    "release-packages": "node scripts/release-packages.js",
    "charts-meta": "node scripts/update-charts-meta.js",
    "typecheck": "tsc --noEmit && pnpm -r typecheck",
    "lint": "eslint ."
  },
  "dependencies": {
    "@types/color": "^3.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0-beta.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.10",
    "@mdit/plugin-include": "^0.12.0",
    "@mdit/plugin-snippet": "^0.12.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it-container": "^2.0.10",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.14.10",
    "@vue/tsconfig": "^0.5.1",
    "@wuxianx/charts": "workspace:^",
    "@wuxianx/charts-vue": "workspace:^",
    "@wuxianx/mock": "workspace:^",
    "bumpp": "^9.4.1",
    "echarts": "^5.5.1",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "lodash-es": "^4.17.21",
    "markdown-it-container": "^4.0.0",
    "minimatch": "^10.0.1",
    "mockjs": "^1.1.0",
    "pnpm": "^9.4.0",
    "rimraf": "^5.0.8",
    "rollup": "^4.19.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "sass": "^1.77.8",
    "simple-git-hooks": "^2.11.1",
    "tdesign-icons-vue-next": "^0.2.4",
    "tdesign-vue-next": "^1.9.8",
    "typescript": "^5.5.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.29"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "*.{vue,ts,tsx}": "pnpm typecheck"
  }
}
